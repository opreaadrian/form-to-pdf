  "devDependencies": {
    "babel-cli": "^6.9.0",
    "babel-register": "^6.9.0",
    "chai": "^3.5.0",
    "mocha": "^2.5.3",
    "nyc": "^6.4.4"
  },
  "dependencies": {
    "winston": "^2.2.0"
